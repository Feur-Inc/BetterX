{
  "manifest_version": 2,
  "name": "Don't overthink 10s",
  "version": "1.0",
  "description": "Add 10 seconds timer thinking before posting",
  "permissions": [
    "https://x.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://x.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
